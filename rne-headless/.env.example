# RNE Headless Configuration

# Connection to ControlBox
BRAIN_ENDPOINT=https://coral-app-x988a.ondigitalocean.app
BRAIN_API_KEY=your-api-key-here

# Stream Configuration
MAX_STREAMS=2
ROTATION_MINUTES=20
CAPTURE_FPS=1

# Browser
HEADLESS=true

# Health Check
HEALTH_PORT=8080

# Logging
LOG_LEVEL=info

# ===========================================
# AI Analysis (GPT-5)
# ===========================================
# Set OPENAI_API_KEY to enable AI analysis
OPENAI_API_KEY=your-openai-key-here

# AI Model (default: gpt-5)
AI_MODEL=gpt-5

# Analyze every Nth frame (controls cost - 10 = analyze every 10th frame)
AI_ANALYZE_EVERY_N=10

# Max tokens per AI response
AI_MAX_TOKENS=1000
